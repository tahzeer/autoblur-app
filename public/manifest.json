{
  "manifest_version": 3,
  "name": "Autoblur",
  "version": "0.1",
  "description": "Automatically blurs input fields on webpages",
  "icons": {
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "options_page": "index.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
